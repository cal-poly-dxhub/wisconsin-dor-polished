{
  "name": "wisconsin-sst",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --filter='*' build",
    "dev": "bun run --filter='*' dev",
    "pytest": "uv run pytest",
    "test": "bun run --elide-lines=0 --filter='*' test",
    "deploy": "bun run bundle && bun run --elide-lines=0 --filter='infra' deploy",
    "destroy": "bun run --elide-lines=0 --filter='infra' destroy",
    "bundle": "uv run scripts/bundle.py",
    "stream": "bash scripts/stream_logs.sh"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.7.9",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.865.0",
    "@aws-sdk/client-sfn": "^3.864.0",
    "@cdklabs/generative-ai-cdk-constructs": "^0.1.312",
    "@tanstack/react-query": "^5.85.5",
    "aws-cdk-lib": "2.208.0",
    "constructs": "^10.0.0",
    "ky": "^1.9.0",
    "node-fetch": "^3.3.2",
    "partysocket": "^1.1.5",
    "react-error-boundary": "^6.0.0",
    "reconnecting-websocket": "^4.4.0",
    "zod": "^4.1.1",
    "zustand": "^5.0.8"
  },
  "engines": {
    "bun": "1.2.19"
  }
}
